<!-- Borrar Modal -->
<div class="modal fade" id="borrarModal" tabindex="-1" role="dialog"
	aria-labelledby="borrarModalLabel" aria-hidden="true">
	<div class="modal-dialog" role="document">
		<div class="modal-content">
			<div class="modal-header">
				<h5 class="modal-title" id="borrarModalLabel">Borrando Elemento</h5>
				<button type="button" class="close" data-dismiss="modal"
					aria-label="Close">
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
			<div class="modal-body">Está intentado BORRAR un Elemento de la lista...
				¿Desea Cancelar o Guardar los Cambios?...</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
				<button type="submit" class="btn btn-primary">Borrar Elemento</button>
			</div>
		</div>
	</div>
</div>